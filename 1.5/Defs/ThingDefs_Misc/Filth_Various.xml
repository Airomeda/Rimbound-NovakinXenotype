<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <ThingDef ParentName="BaseFilth">
    <defName>RB_Filth_PlasmaBlood</defName>
    <label>plasma blood</label>
    <description>A blood puddle of flammable plasma.</description>
    <thingClass>LiquidFuel</thingClass>
    <useHitPoints>true</useHitPoints>
    <tickerType>Normal</tickerType>
    <statBases>
      <Beauty>-10</Beauty>
      <Cleanliness>-15</Cleanliness>
      <MaxHitPoints>100</MaxHitPoints>
      <Flammability>1.5</Flammability>
    </statBases>
    <graphicData>
      <texPath>Things/Filth/LiquidFuel</texPath>
    </graphicData>
    <filth>
      <ignoreFilthMultiplierStat>true</ignoreFilthMultiplierStat>
      <disappearsInDays>25~30</disappearsInDays>
      <rainWashes>true</rainWashes>
      <cleaningWorkToReduceThickness>70</cleaningWorkToReduceThickness>
      <canFilthAttach>true</canFilthAttach>
      <maxThickness>1</maxThickness>
      <cleaningSound>Interact_CleanFilth_Fluid</cleaningSound>
    </filth>
  </ThingDef>

  <ThingDef ParentName="BaseFilth">
    <defName>RB_Filth_PlasmaSmear</defName>
    <label>plasma smear</label>
    <thingClass>LiquidFuel</thingClass>
    <useHitPoints>true</useHitPoints>
    <tickerType>Normal</tickerType>
    <statBases>
      <Beauty>-15</Beauty>
      <Cleanliness>-15</Cleanliness>
      <MaxHitPoints>100</MaxHitPoints>
      <Flammability>1.5</Flammability>
    </statBases>
    <graphicData>
      <texPath>Things/Filth/CrawlSmear</texPath>
      <color>(112, 101, 67, 165)</color>
      <graphicClass>Graphic_ClusterTight</graphicClass>
      <drawSize>0.9</drawSize>
    </graphicData>
    <filth>
      <ignoreFilthMultiplierStat>true</ignoreFilthMultiplierStat>
      <disappearsInDays>25~30</disappearsInDays>
      <rainWashes>true</rainWashes>
      <cleaningWorkToReduceThickness>70</cleaningWorkToReduceThickness>
      <canFilthAttach>true</canFilthAttach>
      <maxThickness>1</maxThickness>
      <cleaningSound>Interact_CleanFilth_Fluid</cleaningSound>
    </filth>
  </ThingDef>

</Defs>
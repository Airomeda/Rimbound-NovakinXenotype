<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <GeneDef>
    <defName>RB_Plasma_Body</defName>
    <label>plasma color bodies</label>
    <labelShortAdj>plasma body</labelShortAdj>
    <description>Carriers of this gene have a plasma color body. With multiple plasma pigment, one is randomly selected.</description>
    <iconPath>UI/Icons/Genes/RB_Gene_PlasmaColor</iconPath>
    <geneClass>RimboundCore.GeneRandomize</geneClass>
    <displayCategory>RB_Novakin_Category</displayCategory>
    <displayOrderInCategory>0</displayOrderInCategory>
    <randomChosen>true</randomChosen>
    <biostatCpx>0</biostatCpx>
    <prerequisite>RB_Novaborn</prerequisite>
    <selectionWeight>0</selectionWeight>
    <exclusionTags>
      <li>SkinColor</li>
      <li>SkinColorOverride</li>
      <li>RB_PlasmaColor</li>
    </exclusionTags>
    <symbolPack>
      <prefixSymbols>
        <li>
          <symbol>plasma</symbol>
        </li>
        <li>
          <symbol>shell</symbol>
        </li>
        <li>
          <symbol>pigment</symbol>
        </li>
      </prefixSymbols>
      <suffixSymbols>
        <li>
          <symbol>plasmic</symbol>
        </li>
        <li>
          <symbol>shellic</symbol>
        </li>
        <li>
          <symbol>pigmentation</symbol>
        </li>
      </suffixSymbols>
    </symbolPack>
    <modExtensions>
      <li Class="RimboundCore.GeneRandomizeExtension">
        <geneDefs>
          <li>RB_Plasma_Blue</li>
          <li>RB_Plasma_Cyan</li>
          <li>RB_Plasma_Green</li>
          <li>RB_Plasma_LightGreen</li>
          <li>RB_Plasma_Orange</li>
          <li>RB_Plasma_Pink</li>
          <li>RB_Plasma_Purple</li>
          <li>RB_Plasma_Red</li>
          <li>RB_Plasma_Yellow</li>
          <li>RB_Plasma_White</li>
        </geneDefs>
      </li>
      <li Class="VanillaGenesExpanded.GeneExtension">
        <backgroundPathEndogenes>UI/Icons/Genes/RB_Endogenes</backgroundPathEndogenes>
        <backgroundPathXenogenes>UI/Icons/Genes/RB_Xenogenes</backgroundPathXenogenes>
        <backgroundPathArchite>UI/Icons/Genes/RB_Archite</backgroundPathArchite>
        <hideGene>true</hideGene>
      </li>
    </modExtensions>
  </GeneDef>

  <GeneDef Name="RB_GenePlasmaColorBase" Abstract="True">
    <biostatCpx>0</biostatCpx>
    <iconPath>UI/Icons/Genes/RB_Gene_PlasmaColor</iconPath>
    <displayCategory>RB_Novakin_Category</displayCategory>
    <displayOrderInCategory>0</displayOrderInCategory>
    <randomBrightnessFactor>0</randomBrightnessFactor>
    <prerequisite>RB_Novaborn</prerequisite>
    <selectionWeight>0</selectionWeight>
    <exclusionTags>
      <li>SkinColor</li>
      <li>SkinColorOverride</li>
      <li>RB_PlasmaColor</li>
    </exclusionTags>
    <modExtensions>
      <li Class="VanillaGenesExpanded.GeneExtension">
        <backgroundPathEndogenes>UI/Icons/Genes/RB_Endogenes</backgroundPathEndogenes>
        <backgroundPathXenogenes>UI/Icons/Genes/RB_Xenogenes</backgroundPathXenogenes>
        <backgroundPathArchite>UI/Icons/Genes/RB_Archite</backgroundPathArchite>
      </li>
      <li Class="RimboundCore.GeneHairSkinColorExtension">
        <hairUsesSkinColor>true</hairUsesSkinColor>
      </li>
    </modExtensions>
  </GeneDef>

  <GeneDef ParentName="RB_GenePlasmaColorBase">
    <defName>RB_Plasma_Blue</defName>
    <label>blue plasma color</label>
    <description>Carriers of this gene produce a pigment that turns their body shell a bright blue color.</description>
    <hairColorOverride>(178, 200, 248)</hairColorOverride>
    <skinColorOverride>(178, 200, 248)</skinColorOverride>
  </GeneDef>


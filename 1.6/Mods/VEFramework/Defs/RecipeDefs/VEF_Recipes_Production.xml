    <jobString>Extracting neutroamine from plasmaskin.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>BurnDrug</effectWorking>
    <soundWorking>Recipe_Drug</soundWorking>
    <workAmount>600</workAmount>
    <recipeUsers>
      <li>DrugLab</li>
      <li>BiofuelRefinery</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RB_Leather_Plasmaskin</li>
          </thingDefs>
        </filter>
        <count>2</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>RB_Leather_Plasmaskin</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Neutroamine>1</Neutroamine>
    </products>
    <skillRequirements>
      <Intellectual>7</Intellectual>
    </skillRequirements>
    <workSkill>Intellectual</workSkill>
    <displayPriority>0</displayPriority>
  </RecipeDef>

